public class Swap {

 public static void main(String[] args) {

  int x =5;
  int y =2;

  System.out.println("Values of x and y before swapping x:"+x+"y:"+y);

  //add both the numbers and assign it to first
  x=x+y;
  y=x-y;
  x=x-y;

  System.out.println("Values of x and y after swapping x:"+x+"y:"+y);
 }
}
